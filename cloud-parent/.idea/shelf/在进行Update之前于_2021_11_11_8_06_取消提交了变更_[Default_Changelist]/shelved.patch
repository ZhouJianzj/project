<+><?xml version=\"1.0\" encoding=\"UTF-8\" ?>\r\n<!DOCTYPE mapper\r\n        PUBLIC \"-//mybatis.org//DTD Mapper 3.0//EN\"\r\n        \"http://mybatis.org/dtd/mybatis-3-mapper.dtd\">\r\n<mapper namespace=\"com.zj.dao.SysDao\">\r\n    <!--    用户登录sql-->\r\n    <resultMap id=\"user_map\" type=\"com.zj.entity.UserManager\">\r\n        <!--        用户信息-->\r\n        <id column=\"id\" property=\"id\"/>\r\n        <result column=\"username\" property=\"username\"/>\r\n        <result column=\"password\" property=\"password\"/>\r\n        <result column=\"salt\" property=\"salt\"/>\r\n        <result column=\"phone\" property=\"phone\"/>\r\n        <!--        用户所在的公司-->\r\n        <association property=\"o\" javaType=\"com.zj.entity.Organize\">\r\n            <id column=\"orgaid\" property=\"id\"></id>\r\n            <result column=\"organame\" property=\"name\"/>\r\n        </association>\r\n        <!--       用户____> 角色表____>角色实体集 -->\r\n        <collection property=\"roles\" ofType=\"com.zj.entity.Role\">\r\n            <id column=\"rid\" property=\"id\"/>\r\n            <result column=\"rolename\" property=\"name\"/>\r\n            <result column=\"ext\" property=\"ext\"/>\r\n            <!--         角色表____>权限表_____>权限实体集-->\r\n            <collection property=\"perms\" ofType=\"com.zj.entity.Perm\">\r\n                <id column=\"pid\" property=\"id\"/>\r\n                <result column=\"permname\" property=\"name\"/>\r\n            </collection>\r\n        </collection>\r\n    </resultMap>\r\n    <select id=\"userSelect\" resultMap=\"user_map\">\r\n        SELECT u.id,\r\n               u.username,\r\n               u.password,\r\n               u.salt,\r\n               u.phone,\r\n               o.NAME AS organame,\r\n               r.id   as rId,\r\n               r.NAME AS rolename,\r\n               r.ext,\r\n               p.id   as pId,\r\n               p.NAME AS permname\r\n        FROM t_user u\r\n                 LEFT JOIN organize o ON o.id = u.orga_id\r\n                 LEFT JOIN t_user_role ur ON ur.userid = u.id\r\n                 LEFT JOIN t_role r ON r.id = ur.roleid\r\n                 LEFT JOIN t_role_perms rp ON rp.role_id = r.id\r\n                 LEFT JOIN t_perms p ON p.id = rp.perm_id\r\n        where u.username = #{username}\r\n          and u.password = #{password}\r\n    </select>\r\n\r\n    <!--    机构查询根据机构名称模糊查询，支持分页，page-->\r\n    <resultMap id=\"orga_type_map\" type=\"com.zj.entity.Organize\">\r\n        <id column=\"id\" property=\"id\"/>\r\n        <result column=\"name\" property=\"name\"/>\r\n        <!--        公司的父公司-->\r\n        <result column=\"parentorganame\" property=\"parentName\"/>\r\n        <result column=\"location\" property=\"location\"/>\r\n        <result column=\"organum\" property=\"orgaNumber\"/>\r\n        <result column=\"ext\" property=\"ext\"/>\r\n        <!--        公司的类型-->\r\n        <association property=\"orgaType\" javaType=\"com.zj.entity.OrgaType\">\r\n            <result column=\"orgatypename\" property=\"name\"/>\r\n        </association>\r\n    </resultMap>\r\n    <select id=\"organizeSelect\" resultMap=\"orga_type_map\">\r\n        SELECT o1.id,\r\n               o1.name,\r\n               o2.name        as parentorganame,\r\n               ot.name        as orgatypename,\r\n               o1.location,\r\n               o1.orga_number as organum,\r\n               o1.ext\r\n        FROM organize o1\r\n                 left JOIN orga_type ot ON ot.id = o1.id\r\n                 left join organize o2 on o2.id = o1.parent_id\r\n        where o1.name like concat(#{orgName}, '%')\r\n    </select>\r\n\r\n    <!--    机构插入-->\r\n    <insert id=\"organizeInsert\">\r\n        insert into organize\r\n            (name, parent_id, type_id, location, orga_number, ext)\r\n        values (#{name}, #{parentId}, #{typeId}, #{location}, #{orgaNumber}, #{ext})\r\n    </insert>\r\n\r\n    <!--    查询角色-->\r\n    <select id=\"roleSelect\" resultType=\"com.zj.entity.Role\">\r\n        select id,\r\n               name,\r\n               ext\r\n        from t_role\r\n        where name like concat('%', #{roleName}, '%')\r\n    </select>\r\n\r\n    <!--    新增角色-->\r\n    <insert id=\"roleInsert\">\r\n        insert into t_role\r\n            (name, ext)\r\n        values (#{name}, #{ext})\r\n    </insert>\r\n\r\n    <!--    查询所用的权限-->\r\n    <select id=\"permSelect\" resultType=\"com.zj.entity.Perm\">\r\n        select id, name,parent_id as parentId,path,ext,icon\r\n        from t_perms\r\n    </select>\r\n\r\n\r\n    <!--    角色权限添加-->\r\n    <insert id=\"rolePermInsert\" parameterType=\"com.zj.entity.RolePerm\">\r\n        insert into t_role_perms\r\n            (role_id, perm_id)\r\n        values (#{roleId}, #{permId})\r\n    </insert>\r\n<!--        删除所有对应的角色权限-->\r\n    <delete id=\"deleteRolePerm\" >\r\n        delete from t_role_perms where role_id = #{roleId}\r\n    </delete>\r\n\r\n\r\n\r\n\r\n\r\n    <!--    删除角色-->\r\n\r\n    <select id=\"rolePermSelect\" resultType=\"com.zj.entity.RolePerm\">\r\n        select role_id as roleId,perm_id as permId\r\n        from t_role_perms\r\n        where role_id = #{id}\r\n    </select>\r\n\r\n    <select id=\"roleUserSelect\" resultType=\"com.zj.entity.UserRole\">\r\n        select userid,roleid\r\n        from t_user_role\r\n        where roleid = #{id}\r\n    </select>\r\n<!--删除role_perm表中对应的关系-->\r\n    <delete id=\"rolePermDelete\">\r\n        delete\r\n        from t_role_perms\r\n        where role_id = #{id}\r\n    </delete>\r\n<!--    删除role_user中对应的关系-->\r\n    <delete id=\"roleUserDelete\">\r\n        delete\r\n        from t_user_role\r\n        where roleid = #{id}\r\n    </delete>\r\n\r\n    <delete id=\"roleDelete\">\r\n        delete\r\n        from t_role\r\n        where id = #{id}\r\n    </delete>\r\n\r\n    <!--    根据手机号或者是用户name模糊查询用户，key为null的时候会查询所有-->\r\n    <select id=\"userKeySelect\" resultMap=\"user_map\">\r\n        SELECT u.id,\r\n               u.username,\r\n               u.password,\r\n               u.salt,\r\n               u.phone,\r\n               o.id   as orgaid,\r\n               o.name AS organame,\r\n               r.id   as rid,\r\n               r.name AS rolename,\r\n               r.ext,\r\n               p.id   as pid,\r\n               p.name AS permname\r\n        FROM t_user u\r\n                 LEFT JOIN organize o ON o.id = u.orga_id\r\n                 LEFT JOIN t_user_role ur ON ur.userid = u.id\r\n                 LEFT JOIN t_role r ON r.id = ur.roleid\r\n                 LEFT JOIN t_role_perms rp ON rp.role_id = r.id\r\n                 LEFT JOIN t_perms p ON p.id = rp.perm_id\r\n        where u.username like concat(concat('%', #{key}), '%')\r\n           or u.phone like concat(concat('%', #{key}), '%')\r\n    </select>\r\n\r\n    <!--    根据id查询用户-->\r\n    <select id=\"userIdSelect\" resultMap=\"user_map\">\r\n        SELECT u.id,\r\n               u.username,\r\n               u.password,\r\n               u.salt,\r\n               u.phone,\r\n               o.id   as orgaid,\r\n               o.name AS organame,\r\n               r.id   as rid,\r\n               r.name AS rolename,\r\n               r.ext,\r\n               p.id   as pid,\r\n               p.name AS permname\r\n        FROM t_user u\r\n                 LEFT JOIN organize o ON o.id = u.orga_id\r\n                 LEFT JOIN t_user_role ur ON ur.userid = u.id\r\n                 LEFT JOIN t_role r ON r.id = ur.roleid\r\n                 LEFT JOIN t_role_perms rp ON rp.role_id = r.id\r\n                 LEFT JOIN t_perms p ON p.id = rp.perm_id\r\n        where u.id = #{id}\r\n    </select>\r\n\r\n    <!--    查询所有的用户-->\r\n    <select id=\"allUserSelect\" resultMap=\"user_map\">\r\n        SELECT u.id,\r\n               u.username,\r\n               u.password,\r\n               u.salt,\r\n               u.phone,\r\n               o.id   as orgaid,\r\n               o.name AS organame,\r\n               r.id   as rid,\r\n               r.name AS rolename,\r\n               r.ext,\r\n               p.id   as pid\r\n               p.name as permname\r\n        FROM t_user u\r\n                 LEFT JOIN organize o ON o.id = u.orga_id\r\n                 LEFT JOIN t_user_role ur ON ur.userid = u.id\r\n                 LEFT JOIN t_role r ON r.id = ur.roleid\r\n                 LEFT JOIN t_role_perms rp ON rp.role_id = r.id\r\n                 LEFT JOIN t_perms p ON p.id = rp.perm_id\r\n    </select>\r\n\r\n\r\n    <!--    新增用户-->\r\n    <insert id=\"userManagerInsert\">\r\n        insert into t_user (username, password, salt, phone, orga_id)\r\n        values (#{username}, #{password}, #{salt}, #{phone}, #{orgaId})\r\n    </insert>\r\n    <select id=\"useridGet\" resultType=\"Integer\">\r\n        SELECT @@IDENTITY\r\n    </select>\r\n    <insert id=\"userRoleInsert\">\r\n        insert into t_user_role (userid, roleid)\r\n        values (#{userid}, #{roleid})\r\n    </insert>\r\n\r\n\r\n    <!--    删除用户-->\r\n    <delete id=\"userDelete\">\r\n        delete\r\n        from t_user\r\n        where id = #{id}\r\n    </delete>\r\n    <!-- 删除用户（用户拥有角色）-->\r\n    <delete id=\"userRoleDelete\">\r\n        delete\r\n        from t_user_role\r\n        where userid = #{id}\r\n    </delete>\r\n\r\n\r\n    <!--    日志查询-->\r\n    <select id=\"logSelect\" resultType=\"com.zj.entity.Log\">\r\n        select id,\r\n               username,\r\n               operType,\r\n               moduleName,\r\n               result,\r\n               operTimer,\r\n               operContent\r\n        from log\r\n    </select>\r\n\r\n\r\n    <!--    更新用户根据id-->\r\n    <update id=\"userUpdate\" parameterType=\"com.zj.entity.User\">\r\n        update t_user\r\n        set username = #{username},\r\n            phone    = #{phone}\r\n        where id = #{id}\r\n    </update>\r\n    <!--   往user_role表中插入数据-->\r\n    <insert id=\"userInsertRole\" parameterType=\"com.zj.entity.User\">\r\n        insert into t_user_role (userid, roleid)\r\n        values (#{id}, #{roleId})\r\n    </insert>\r\n    <!--   先查询user_role中是否已经存在关系-->\r\n    <select id=\"userSelectRole\" parameterType=\"com.zj.entity.User\" resultType=\"com.zj.entity.UserRole\">\r\n        select *\r\n        from t_user_role\r\n        where userid = #{id}\r\n          and roleid = #{roleId}\r\n    </select>\r\n\r\n    <update id=\"userUpdateOrgaId\">\r\n        update t_user\r\n        set orga_id = #{orgaId}\r\n        where id = #{id}\r\n    </update>\r\n\r\n\r\n\r\n\r\n    <!--    修改密码-->\r\n    <update id=\"passwordModify\">\r\n        update t_user\r\n        set password = #{password}\r\n        where id = #{id}\r\n    </update>\r\n</mapper>
